<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>ranwen's blog</title>
    <link>https://ranwen.de/</link>
    <description>随便写写</description>
    <language>zh-hans-cn</language>
    <lastBuildDate><%= new Date().toISOString() %></lastBuildDate>
    <atom:link href="https://ranwen.de/feed/" rel="self" type="application/rss+xml" />
    <% for (const post of posts) { %>
      <item>
        <title><%= post.title %></title>
        <link>https://ranwen.de<%- post.url %></link>
        <guid>https://ranwen.de<%- post.url %></guid>
        <pubDate><%= new Date(post.date).toISOString() %></pubDate>
        <description><![CDATA[ <%- post.head %> ]]></description>
      </item>
    <% } %>
  </channel>
</rss>